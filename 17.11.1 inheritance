package myjava;

class Tax{
	double grossIncome;
	String state;
	int dependents;
	public double calcTax() {
		return 234.55;
}
}
class Tax2 extends Tax{
	int ss;
}
public class source {
	public static void main(String[] args) {
		Tax a = new Tax();
		a.grossIncome=50000;
		a.dependents=2;
		a.state="nj";
		double yourtax=a.calcTax();
		Tax2 b= new Tax2();
		b.ss=12;
		b.dependents=222;
		System.out.println("Your tax is"+yourtax);
	}

}
